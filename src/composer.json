{
  "name" : "miniHabit",
  "description": "tars http server for miniHabit",
  "require": {
    "php": "^7.0",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-curl": "*",
    "phptars/tars-server": "~0.2.0",
    "phptars/tars-deploy": "~0.1.0",
    "phptars/tars2php": "~0.1.0",
    "phptars/tars-log": "~0.1.0",
    "ext-zip" : ">=0.0.1",
    "illuminate/routing": "~5.5.0",
    "illuminate/events": "~5.5.0",
    "catfan/medoo": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "HttpServer\\" : "./",
      "App\\": "./app/"
    }
  },
  "minimum-stability": "stable",
  "scripts" : {
    "deploy" : "\\Tars\\deploy\\Deploy::run"
  }
}
